// brute force approach
fun main(args: Array<String>) {
  val arr = intArrayOf(1,2,3,4,5)
  var max = Int.MIN_VALUE
  val temp = mutableListOf<Int>()

  for (i in arr.indices){
    var sum = 0
    for (j in i..< arr.size){
      sum += arr[j]
      if (sum > max){
        max = sum
        temp.add(arr[j])

      }
    }
  }
  println("$temp -> $max")

}
